<actionGroups xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:noNamespaceSchemaLocation="urn:magento:mftf:Test/etc/actionGroupSchema.xsd">
    <actionGroup name="AdminNavigateToSeoRobotsConfigActionGroup">
        <annotations>
            <description>Navigates to the SEO Robots configuration page in admin.</description>
        </annotations>

        <amOnPage url="{{AdminSeoRobotsConfigPage.url}}" stepKey="navigateToSeoRobotsConfig"/>
        <waitForPageLoad stepKey="waitForPageLoad"/>
    </actionGroup>
</actionGroups>
